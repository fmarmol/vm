addthree: # this function take 3 arguments and returns a + (b - c)
    swap 2
    dup 3
    swap 2
    sub
    swap 2
    swap 4
    add
    swap 2
    drop
    swap 2
    ret
// initial stack
// ip
// -1
__start:
    pushi 3 # 3th
    pushi 4 # 2nd
    pushi 8 # 1st
    call addthree
    eqi -1
    halt
